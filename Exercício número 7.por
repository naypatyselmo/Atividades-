programa {
  funcao inicio() {
    real a, b, c, delta

        escreva("Digite o valor de a: ")
        leia(a)

        escreva("Digite o valor de b: ")
        leia(b)

        escreva("Digite o valor de c: ")
        leia(c)

        delta = (b * b) - (4 * a * c)

        escreva("O valor de Δ (delta) é: ", delta)
  }
}

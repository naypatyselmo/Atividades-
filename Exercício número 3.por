programa {
  funcao inicio() {
    inteiro num1, num2, num3

    // Entrada de dados
        escreva("Digite o primeiro número (menor): ")
        leia(num1)

        escreva("Digite o segundo número (médio): ")
        leia(num2)

        escreva("Digite o terceiro número (maior): ")
        leia(num3)

        // Impressão em ordem decrescente
        escreva("Números em ordem decrescente: ", num3, ", ", num2, ", ", num1)
  }
}
